function matlab() {
    open -a "Matlab" "$@"
}

# pushes .zshrc to my Misc git repo
function zshrcgit() {
    cd "${HOME}/Documents/Projects/System"
    cp "${HOME}/.zshrc" .
    cp "${HOME}/.zsh_aliases" .
    cp "${HOME}/.zsh_functions" . 
    git add .zshrc
    git add .zsh_aliases
    git add .zsh_functions
    if [ -z "$1" ]; then
        git commit 
    else
        git commit -m "$1"
    fi
    git push
    cd "$OLDPWD"
}

